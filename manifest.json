{
    "name": "Try Themes",
    "version": "0.1.0",
    "manifest_version": 2,
    "icons": {"512": "/assets/trythemes.png"},
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "css": ["styles.css"]
      }
    ],
    "browser_action": {
      "default_title": "Try Themes",
      "default_popup":"popup.html"
    }
  }